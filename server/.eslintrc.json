{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    // "no-console": [
    //   "warn",
    //   {
    //     "allow": [
    //       "error"
    //     ]
    //   }
    // ],
    "quotes": [
      "error",
      "single"
    ],
    "no-duplicate-imports": "error",
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "args": "all"
      }
    ]
  }
}